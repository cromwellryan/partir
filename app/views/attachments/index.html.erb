<p id="notice"><%= notice %></p>

<h1>Attachments for deploy <%=@deploy.id %> of <%=@project.name%> <%=time_ago_in_words @deploy.occurred_at%> ago</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attachments.each do |attachment| %>
      <tr>
        <td><%= render attachment%></td>
        <td><%= link_to 'Show', [@project, @deploy, attachment] %></td>
        <td><%= link_to 'Destroy', [@project, @deploy, attachment], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

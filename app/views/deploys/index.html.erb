<p id="notice"><%= notice %></p>

<h1>Deploys for <%=@project.name%></h1>

<table>
  <thead>
    <tr>
      <th>Occurred</th>
      <th>SHA</th>
      <th>Environment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deploys.each do |deploy| %>
      <tr>
        <td><%= deploy.occurred_at%></td>
        <td><%= deploy.sha %></td>
        <td><%= deploy.environment%></td>
        <td><%= link_to 'Show', [@project, deploy] %></td>
        <td><%= link_to 'Destroy', [@project, deploy], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
